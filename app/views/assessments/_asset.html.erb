<tr id="<%= dom_id(assessment) %>" class="assessment <%= cycle('even', 'odd') %>">
  <td><span class="title"><%=h title(assessment.title) %></span></td>
  <td><span class="category"><%=h title(assessment.category.name) %></span></td>
  <td><%=h assessment.year %></td>
  <td><%=h assessment.author %></td>
  <td>
    <%= link_to "Vaata", assessment %>
    <% if admin? -%>
      | <%= link_to "Kustuta", '#', :onclick => 'destroyAsset(' + assessment.id.to_s + ');' %>
    <% end -%>
  </td>     
</tr>
