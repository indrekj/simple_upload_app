<% case @asset.type %>
<% when "document" %>
  <% if preview_url = preview_url(@asset) %>
    <iframe style="width:850px; height:500px" 
      frameborder="0" 
      src="http://docs.google.com/viewer?embedded=true&url=<%= preview_url %>">
    </iframe>
  <% end %>
<% when "note" %>
  <%=raw contents(@asset) %>
